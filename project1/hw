#game guess the number
print (“I’m thinking of a number between 1 and 20. Guess it”.)
import random
correct_number = random.randint(1, 20) 
guess_number = int(input())
x_tries = 1

while x_tries >= 6
	while guess_number != correct_number:
		if guess_number < correct number:
			print(Too low. Try again!”)
		else:
			print (Too high. Try again!”)
		guess_number = int(input())
		x_tries + 1
	print (“You got it! Nice!”)
print(“you’re all out of tries”)
